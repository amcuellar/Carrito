<?xml version="1.0" encoding="UTF-8"?>
<faces-config
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	version="2.2">
	<managed-bean>
		<managed-bean-name>crearAnimalMB</managed-bean-name>
		<managed-bean-class>co.asw.presentacion.CrearAnimalMB</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>indexMB1</managed-bean-name>
		<managed-bean-class>co.asw.presentacion.IndexMB</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
	</managed-bean>

	<navigation-rule>
		<display-name>index.xhtml</display-name>
		<from-view-id>/index.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>prueba</from-outcome>

			<to-view-id>/prueba.xhtml</to-view-id>

		</navigation-case>

	</navigation-rule>

</faces-config>